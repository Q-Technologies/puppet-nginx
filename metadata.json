{
  "name": "qtechnologies/nginx",
  "version": "1.1.1",
  "author": "Q-Technologies",
  "summary": "Puppet module to manage standard web application configuration in Nginx",
  "license": "Apache-2.0",
  "tags": ["nginx","php-fpm","opencart","psgi","perl Dancer","owncloud","php","letsencrypt","let's encrypt"],
  "source": "https://github.com/Q-Technologies/puppet-nginx.git",
  "project_page": "https://github.com/Q-Technologies/puppet-nginx",
